TYPE construction_graph
INPUT 0
BODY
	create_graph ugraph g0 0
	assert normalised g0
	assert num_edges g0 0
	output_graph g0
	
	create_graph ugraph g1 5
	add_edge g1 0 2 false
	add_edge g1 0 1 false
	assert num_nodes g1 5
	assert num_edges g1 2
	assert not_normalised g1
	normalise g1
	assert normalised g1
	output_graph g1
	
	create_graph ugraph g2 0
	assign g2 g1
	assert equal_graphs g1 g2
	assert equal_graphs g2 g1
	assert num_nodes g2 5
	assert num_edges g2 2
	assert normalised g2
	assert equal_graphs g1 g2
	assert equal_graphs g2 g1
	output_graph g2
	
	create_graph ugraph g3 10
	add_edge g3 0 1 true
	assign g3 g1
	assert equal_graphs g1 g3
	assert equal_graphs g3 g1
	output_graph g3
	
	create_graph ugraph g4 6
	add_edge g4 5 0 false
	add_edge g4 4 0 false
	add_edge g4 3 5 false
	add_edge g4 1 0 false
	add_edge g4 2 5 false
	add_edge g4 0 2 false
	add_edge g4 3 0 false
	output_graph g4
	assert not_normalised g4
	normalise g4
	assert normalised g4
	output_graph g4
