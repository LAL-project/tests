TYPE construction_graph
INPUT 0
BODY
	create_graph undirected g1 10
	add_edge g1 0 2 false
	add_edge g1 0 1 false
	assert num_edges g1 2
	assert not_normalised g1
	normalise g1
	assert normalised g1
	
	add_edge g1 5 6 true
	add_edge g1 0 5 true
	assert normalised g1

	assert exists_edge g1 0 2
	assert exists_edge g1 2 0
	assert exists_edge g1 0 1
	assert exists_edge g1 1 0
