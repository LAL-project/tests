TYPE construction_graph
INPUT 0
BODY
	create_graph ugraph g 10
	
	add_edge g 0 1 false
	assert exists_edge g 1 0
	remove_edge g 1 0 false
	assert not_exists_edge g 0 1
	
	add_edge g 0 6 false
	add_edge g 0 1 false
	add_edge g 0 3 false
	add_edge g 0 2 false
	add_edge g 0 4 false
	add_edge g 0 5 false
	output_graph g
	
	remove_edges g 2
		0 1
		0 2
		false
	output_graph g
	assert not_normalised g
	
	
	create_graph ugraph f 10
	
	add_edge f 0 1 false
	assert exists_edge f 1 0
	remove_edge f 1 0 false
	assert not_exists_edge f 0 1
	
	add_edge f 0 6 false
	add_edge f 0 1 false
	add_edge f 0 3 false
	add_edge f 0 2 false
	add_edge f 0 4 false
	add_edge f 0 5 false
	output_graph f
	
	remove_edges f 2
		0 1
		0 2
		true
	output_graph f
	assert normalised f
	
	
	
	create_graph ugraph h 10
	
	add_edge h 0 1 false
	assert exists_edge h 1 0
	remove_edge h 1 0 false
	assert not_exists_edge h 0 1
	
	add_edge h 0 6 false
	add_edge h 0 1 false
	add_edge h 0 3 false
	add_edge h 0 2 false
	add_edge h 0 4 false
	add_edge h 0 5 false
	output_graph h
	
	remove_edges h 2
		0 6
		0 3
		false
	output_graph h
	assert normalised h
