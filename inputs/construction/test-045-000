TYPE construction_graph
INPUT 0
BODY
	create_graph ugraph g0 0
	to_drtree dg0 g0 0
	output_graph dg0
	
	create_graph ugraph g1 1
	to_drtree dg1 g1 0
	output_graph dg1
	
	create_graph ugraph g2 2
	assert num_nodes g2 2
	assert num_edges g2 0
	add_edge g2 0 1 false
	assert num_edges g2 1
	output_graph g2
