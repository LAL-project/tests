TYPE construction_graph
INPUT 0
BODY
	create_graph undirected g 5
	
	assert num_edges g 0
	assert edges_are g 0
	
	add_edge g 0 4 false
	assert num_edges g 1
	assert edges_are g 1
		0 4
	
	add_edge g 0 3 false
	assert num_edges g 2
	assert edges_are g 2
		0 4
		0 3
	
	add_edge g 1 2 false
	assert num_edges g 3
	assert edges_are g 3
		0 4
		0 3
		1 2
	
	add_edge g 0 2 false
	assert num_edges g 4
	assert edges_are g 4
		0 4
		0 3
		1 2
		0 2
	
	add_edge g 0 1 false
	assert num_edges g 5
	assert edges_are g 5
		0 4
		0 3
		1 2
		0 2
		0 1
