TYPE construction_graph
INPUT 0
BODY
	create_graph directed g1 10
	add_edge g1 0 2 false
	add_edge g1 1 0 false
	add_edge g1 0 1 false
	add_edge g1 2 0 false
	assert not_normalised g1
	add_edge g1 5 6 true
	assert normalised g1
	add_edge g1 5 0 true
	add_edge g1 0 5 true
	add_edge g1 6 5 true
	add_edge g1 3 4 false
	assert normalised g1
	add_edge g1 5 4 false
	assert not_normalised g1
	normalise g1
	assert normalised g1
	add_edge g1 0 6 false
	add_edge g1 0 7 false
	add_edge g1 0 8 false
	add_edge g1 0 9 false
	assert normalised g1
	add_edge g1 0 3 false
	add_edge g1 0 4 false
	assert not_normalised g1

	assert exists_edge g1 0 2
	assert exists_edge g1 2 0
	assert exists_edge g1 0 1
	assert exists_edge g1 1 0
	
	assert exists_edge g1 5 6
	assert exists_edge g1 6 5
	assert exists_edge g1 0 5
	assert exists_edge g1 5 0
	
	assert edges_are g1
		16
		0 2
		1 0
		0 1
		2 0
